<?xml version="1.0" encoding="utf-8" ?><extension xmlns="http://ns.adobe.com/air/extension/18.0">    <id>eu.amodo.extension</id>    <versionNumber>0.0.1</versionNumber>    <platforms>        <platform name="Android-ARM">            <applicationDeployment>                <nativeLibrary>Extension.jar</nativeLibrary>                <initializer>eu.amodo.extension.ExtensionActivity</initializer>                 <finalizer>eu.amodo.extension.ExtensionActivity</finalizer>            </applicationDeployment>        </platform>        <platform name="iPhone-ARM">            <applicationDeployment>                <nativeLibrary>libExtension.a</nativeLibrary>                <initializer>ExtensionExtInitializer</initializer>                <finalizer>ExtensionExtFinalizer</finalizer>            </applicationDeployment>        </platform>		<platform name="default">            <applicationDeployment/>        </platform>    </platforms></extension>